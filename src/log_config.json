{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "[%(levelname)s|%(name)s|%(module)s]: %(message)s"
        },
        "json": {
            "format": "{\"level\": \"%(levelname)s\", \"message\": \"%(message)s\", \"timestamp\": \"%(asctime)s\", \"logger\": \"%(name)s\", \"module\": \"%(module)s\", \"function\": \"%(funcName)s\", \"line\": \"%(lineno)d\", \"thread_name\": \"%(threadName)s\"}",
            "datefmt": "%Y-%m-%dT%H:%M:%S%z"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        },
        "system_file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "json",
            "filename": "logs/system.log.jsonl",
            "encoding": "utf-8",
            "maxBytes": 5242880,
            "backupCount": 1
        },
        "model_file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "json",
            "filename": "logs/model.log.jsonl",
            "encoding": "utf-8",
            "maxBytes": 5242880,
            "backupCount": 1
        }
    },
    "loggers": {
        "system": {
            "level": "DEBUG",
            "handlers": ["console", "system_file"],
            "propagate": false
        },
        "model": {
            "level": "DEBUG",
            "handlers": ["model_file"],
            "propagate": false
        },
        "model.collect": {
            "level": "DEBUG",
            "handlers": ["model_file"],
            "propagate": false
        },
        "model.channel": {
            "level": "DEBUG",
            "handlers": [
                "model_queue"
            ],
            "propagate": false
        }
    },
    "root": {
        "level": "INFO",
        "handlers": ["console"]
    }
}